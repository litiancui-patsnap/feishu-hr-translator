APP_PORT=8080
APP_BASE_URL=http://localhost:8080

# Feishu Bot / App
FEISHU_APP_ID=your_app_id
FEISHU_APP_SECRET=your_app_secret
FEISHU_BOT_VERIFICATION_TOKEN=your_verification_token
FEISHU_BOT_ENCRYPT_KEY=your_encrypt_key
FEISHU_DEFAULT_CHAT_ID=

# Qwen (DashScope)
DASHSCOPE_API_KEY=your_dashscope_key
QWEN_MODEL=qwen-max
QWEN_API_MODE=text

# Storage selector: csv|sheet|bitable
STORAGE_DRIVER=csv
CSV_PATH=./data/reports_slim.csv

# Google Sheet (if STORAGE_DRIVER=sheet)
GOOGLE_SERVICE_ACCOUNT_JSON=./secrets/gs.json
GOOGLE_SHEET_ID=

# Bitable (if STORAGE_DRIVER=bitable)
FEISHU_TENANT_KEY=
BITABLE_BASE_ID=
BITABLE_TABLE_ID=

# OKR source: cache|sheet|bitable
OKR_SOURCE=cache
OKR_CACHE_PATH=./data/okr_cache.json

# (Optional) Feishu tenant for OKR sync job
FEISHU_TENANT_APP_ID=
FEISHU_TENANT_APP_SECRET=
FEISHU_OKR_IDS=
FEISHU_OKR_OWNER_OVERRIDES=        # ∏Ò Ω okr_id:user_id;okr_id2:user_id2

# Networking
REQUEST_TIMEOUT_SECONDS=10
HTTP_TRUST_ENV=false
